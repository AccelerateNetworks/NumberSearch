@model NumberSearch.DataAccess.OwnedPhoneNumber
@{
    ViewData["Title"] = "Owned Phone Number";
}
<br />
<h1 class="display-4">Owned Phone Number</h1>
<br />
<div class="pb-3">
    <ul class="nav nav-pills nav-fill nav-justified">
        <li class="nav-item">
            <a class="nav-link" asp-controller="Home" asp-action="Numbers">Numbers</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" asp-controller="Home" asp-action="NumberOrders">Number Orders</a>
        </li>
        <li class="nav-item">
            <a class="nav-link active" asp-controller="Home" asp-action="OwnedNumbers">Owned Numbers</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" asp-controller="Home" asp-action="Emails">Sent Emails</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" asp-controller="Home" asp-action="PortRequests">Port Requests</a>
        </li>
    </ul>
</div>
<form href="Home/OwnedNumbers/@Model.DialedNumber" method="post" class="needs-validation">
    <div class="row">
        <div class="col-md-6 mb-3">
            <label for="firstName">Dialed Number</label>
            <input asp-for="DialedNumber" type="text" class="form-control" id="DialedNumber" readonly>
        </div>
        <div class="col-md-6 mb-3">
            <label for="lastName">Active</label>
            <input asp-for="Active" type="checkbox" class="form-control" id="Active" required>
        </div>
    </div>
    <div class="row">
        <div class="col-md-6 mb-3">
            <label for="firstName">Ingested From</label>
            <input asp-for="IngestedFrom" type="text" class="form-control" id="IngestedFrom" readonly>
        </div>
        <div class="col-md-6 mb-3">
            <label for="lastName">Ingested Date</label>
            <input asp-for="DateIngested" type="text" class="form-control" id="DateIngested" readonly>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 mb-3">
            <label for="firstName">Notes</label>
            <input asp-for="Notes" type="text" class="form-control" id="Notes">
        </div>
    </div>
    <div class="row">
        <div class="col-md-6 mb-3">
            <label for="lastName">Service Provider</label>
            <input asp-for="SPID" type="text" class="form-control" id="SPID" readonly>
        </div>
        <div class="col-md-6 mb-3">
            <label for="lastName">Service Provider Id</label>
            <input asp-for="SPID" type="text" class="form-control" id="SPID" readonly>
        </div>
    </div>
    <div class="row">
        <div class="col-md-6 mb-3">
            <label for="firstName">Billing Client Id</label>
            <input asp-for="BillingClientId" type="text" class="form-control" id="BillingClientId">
        </div>
        <div class="col-md-6 mb-3">
            <label for="lastName">Owned By</label>
            <input asp-for="OwnedBy" type="text" class="form-control" id="OwnedBy">
        </div>
    </div>
    <button type="submit" class="btn btn-danger">Update</button>
    <br />
</form>
<br />