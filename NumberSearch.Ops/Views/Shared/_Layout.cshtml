<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] | Accelerate Networks</title>
    <link rel="icon" href="~/images/scaled/favicon-194x194.png">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <div class="pt-3">
        <div class="container">
            <div class="row">
                <div class="col-xl-3 col-md-3">
                    <a asp-controller="Home" asp-action="Index" class="navbar-brand mx-auto font-weight-normal">
                        <img src="~/images/scaled/accelerate.png" alt="Accelerate Networks" class="img-fluid" style="width: 10rem;">
                    </a>
                </div>
                <div class="col-xl-9 col-md-9 d-flex flex-wrap justify-content-around align-items-center">
                    <a class="p-2 text-dark" asp-controller="Home" asp-action="Orders">Orders</a>
                    <a class="p-2 text-dark" asp-controller="Home" asp-action="PortRequests">Port Requests</a>
                    <a class="p-2 text-dark" asp-controller="Home" asp-action="Numbers">Numbers</a>
                    <a class="p-2 text-dark" asp-controller="Home" asp-action="NumberOrders">Purchased</a>
                    @*<a class="p-2 text-dark" asp-controller="Home" asp-action="Support">Support</a>
                        <a class="p-2 text-dark" asp-controller="Home" asp-action="About">About</a>*@
                    @*<a class="p-2 text-dark" href="https://blog.acceleratenetworks.com/" target="_blank"
                        rel="noopener noreferrer">Blog</a>*@
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </div>
    </div>
    <main role="main" class="container">
        @RenderBody()
    </main>

    <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
